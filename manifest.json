{
  "manifest_version": 3,
  "name": "Browsing-history",
  "description": "实现浏览器历史记录的查看和统计",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "history",
    "downloads",
    "favicon"
  ],

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-64.png",
    "128": "images/icon-128.png"
  },

  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],

  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ],
  
  "action": {
    "default_popup": "history.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-64.png",
      "128": "images/icon-128.png"
    }
  }
}